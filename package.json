{
  "name": "elven-goods-market",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "firebase emulators:exec \"ng serve\"",
    "build": "ng build",
    "watch": "firebase emulators:exec \"ng build --watch --configuration development\"",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=FirefoxDeveloperHeadless",
    "lint": "ng lint",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,html,scss}": [
      "npm run format"
    ],
    "*.{json,md}": [
      "npm run format"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.6",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "~13.3.6",
    "@angular/compiler": "~13.3.6",
    "@angular/core": "~13.3.6",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.3.6",
    "@angular/material": "^13.3.6",
    "@angular/platform-browser": "~13.3.6",
    "@angular/platform-browser-dynamic": "~13.3.6",
    "@angular/router": "~13.3.6",
    "firebase": "^9.6.7",
    "rxfire": "^6.0.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.6",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.46",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.9.0",
    "firebase-tools": "^9.23.3",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "~4.4.3"
  }
}
